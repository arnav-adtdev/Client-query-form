

  {
    "version": 2,
    "builds": [
      {
        "src": "views/**/*.pug",
        "use": "@vercel/static-build"
      },
      {
        "src": "/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  